#: dat/missions/zalek/neburesearch/neburesearch_01.lua:39
msgid "Escort the transport ship to {pnt} in the {sys} system"
msgstr ""

#: dat/missions/zalek/neburesearch/neburesearch_01.lua:40 dat/missions/zalek/neburesearch/neburesearch_04.lua:79
#: dat/missions/zalek/neburesearch/neburesearch_04.lua:91
#: dat/missions/zalek/neburesearch/neburesearch_05.lua:102
