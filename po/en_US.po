msgid "We are under attack!"
msgstr ""

#: dat/missions/minerva/pirate4.lua:284
msgid "We have been discovered!"
msgstr ""

#: dat/missions/minerva/pirate4.lua:290
msgid "The interrogation ship was destroyed!"
msgstr ""

#. Get ready!
#: dat/missions/minerva/pirate4.lua:295
msgid "Reinforcements are coming!"
msgstr ""

