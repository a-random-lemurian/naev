#. Must be accepted beyond this point
#. mission details
#: dat/missions/zalek/particlephysics/zpp01.lua:90 dat/missions/zalek/particlephysics/zpp01.lua:96
#: dat/missions/zalek/particlephysics/zpp02.lua:79 dat/missions/zalek/particlephysics/zpp02.lua:86
#: dat/missions/zalek/particlephysics/zpp03.lua:74 dat/missions/zalek/particlephysics/zpp03.lua:81
