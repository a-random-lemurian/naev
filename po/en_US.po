#: dat/missions/neutral/runaway/runaway_search.lua:89 dat/missions/neutral/runaway/runaway_search.lua:95
#: dat/missions/neutral/runaway/runaway_search.lua:107 dat/missions/neutral/runaway/runaway_search.lua:110
